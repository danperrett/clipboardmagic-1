            if(m_ptzClient != null)
            {
                if (movement < 0)
                {
                    m_ptzClient.Pan(PanAction.Left, 20);
                }
                else
                {
                    m_ptzClient.Pan(PanAction.Right, 20);
                }
                int seconds = (movement < 0)? (-1*movement):movement;

                Thread.Sleep(seconds * 1000);
                m_ptzClient.Pan(PanAction.Stop, 10);
